{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "React",
            "cwd": "${workspaceRoot}/reactui",
            "runtimeExecutable": "${workspaceRoot}/reactui/node_modules/.bin/react-scripts",
            "args": [
                "start",
                "--nolazy",
                "--no-cache",
                "--env=jsdom"
            ],
            "protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Electron",
            "cwd": "${workspaceRoot}/electron",
            "runtimeExecutable": "${workspaceRoot}/electron/node_modules/.bin/electron",
            "windows": {
              "runtimeExecutable": "${workspaceRoot}/electron/node_modules/.bin/electron.cmd"
            },
            "args": [
                "."
            ]
        }
    ]
}